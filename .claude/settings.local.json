{
  "permissions": {
    "allow": [
      "Bash",
      "WebSearch",
      "WebFetch(domain:docs.rs)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:docs.docker.com)",
      "WebFetch(domain:platform.openai.com)",
      "WebFetch(domain:lmstudio.ai)"
    ],
    "deny": [
      "Bash(rm -rf :*)",
      "Bash(rm -r :*)",
      "Bash(rm -f :*)",
      "Bash(rmdir :*)",
      "Bash(git push --force:*)",
      "Bash(git reset --hard:*)",
      "Bash(git clean -fd:*)",
      "Bash(chmod -R 777:*)",
      "Bash(dd :*)",
      "Bash(mkfs:*)"
    ],
    "ask": []
  }
}
